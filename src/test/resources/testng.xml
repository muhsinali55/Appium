<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="AppiumSuite" verbose="1">

    <listeners>
        <listener class-name="appium.ExtentListener"/>
    </listeners>

    <test name="SmokeTests">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>

        <classes>
            <class name="appium.eCommerce"/>
        </classes>
    </test>

</suite>
